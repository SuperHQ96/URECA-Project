<% include ../../../partials/header %>
<% include ../../../partials/navbar %>
<div class="container">
  <h1>Add Due Date on <%= date %> to paper</h1>
  <form action="#" method="post">
    <div class="form-group row">
      <div class="col-xs-4">
        <h2>Choose a paper: </h2>
        <select class="form-control" name="course_code">
          <% papers.forEach(function(paper){ %>
            <option value="<%= paper.paperID %>"><%= paper.course_code %> (Y<%= paper.paperYear %> Semester <%= paper.paperSem %>)</option>
          <% }) %>
        </select>
      </div>
    </div>
  </br>
  <div class="form-group">
      <input class="btn btn-lg btn-primary" type="submit" value="Add">
  </div>
  </form>
</div>
</body>
</html>

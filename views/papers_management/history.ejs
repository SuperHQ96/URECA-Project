<% include ../../partials/header %>
<% include ../../partials/navbar %>
  <div class="container">
    <h1>Please Select a paper:</h1>
    <form action="/paper_log" method="post">
      <div class="form-group row">
        <div class="col-md-4">
          <select class="form-control" name="paperID">
            <% papers.forEach(function(paper){ %>
              <option value="<%= paper.paperID %>"><%= paper.course_code %> (Y<%= paper.paperYear %> Semester <%= paper.paperSem %>)</option>
            <% }) %>
          </select>
        </div>
      </div>
    </br>
    <div class="form-group">
        <input class="btn btn-lg btn-primary" type="submit" value="Select">
    </div>
    </form>
  </div>
</body>
</html>

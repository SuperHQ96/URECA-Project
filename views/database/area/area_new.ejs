<% include ../../../partials/header %>
<% include ../../../partials/navbar %>
  <div class="container">
    <h1><b>Insert new area record</b></h1>
    </br>
    <form  action="/area/new" method="post">
      <div class="form-group row">
          <div class="col-xs-8">
            <h2>Area Code</h2>
            <input class="form-control" type="text" name="area_code" placeholder="Area code" required>
          </div>
      </div>
      <div class="form-group row">
          <div class="col-xs-8">
            <h2>Area Lead</h2>
            <input class="form-control" type="text" name="area_lead" placeholder="Area lead" required>
          </div>
      </div>
      <div class="form-group row">
          <div class="col-xs-8">
            <h2>Area Lead Email</h2>
            <input class="form-control" type="text" name="area_lead_email" placeholder="Area lead email" required>
          </div>
      </div>
      <div class="form-group row">
          <div class="col-xs-8">
            <h2>Area Admin</h2>
            <input class="form-control" type="text" name="area_admin" placeholder="Area admin" required>
          </div>
      </div>
      <div class="form-group row">
          <div class="col-xs-8">
            <h2>Area Admin Email</h2>
            <input class="form-control" type="text" name="area_admin_email" placeholder="Area admin email" required>
          </div>
      </div>
      </br>
      <div class="form-group">
          <input class="btn btn-lg btn-primary" type="submit" value="Insert Record">
      </div>
    </form>
  </div>


  </body>
</html>

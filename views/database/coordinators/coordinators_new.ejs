<% include ../../../partials/header %>
<% include ../../../partials/navbar %>
  <div class="container">
    <% if(error_message) { %>
      <div class="alert alert-danger" role="alert">
        <%= error_message %>
      </div>
    <% } %>
    <h1><b>Insert new coordinator</b></h1>
    </br>
    <form  action="/coordinator/new" method="post">
      <div class="form-group row">
        <div class="col-xs-4">
          <h2>Course Code</h2>
          <select class="form-control" name="course_code">
            <% course_codes.forEach(function(course_code){ %>
              <option value="<%= course_code.course_code %>"><%= course_code.course_code %></option>
            <% }) %>
          </select>
        </div>
      </div>
      <div class="form-group row">
        <div class="col-xs-4">
          <h2>Initial</h2>
          <select class="form-control" name="initial">
            <% initials.forEach(function(initial){ %>
              <option value="<%= initial.initial %>"><%= initial.initial %></option>
            <% }) %>
          </select>
        </div>
      </div>
      </br>
      <div class="form-group">
          <input class="btn btn-lg btn-primary" type="submit" value="Insert Record">
      </div>
    </form>
  </div>


  </body>
</html>
